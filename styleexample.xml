<?xml version="1.0" encoding="utf-8"?>
<style name="moon">
  
  <line id="border" color="white"></line>

  <button id="button_txt">
    <framed>
      <text col="white"></text>
    </framed>
    <framed>
      <box color="darkgreen"></box>
      <text col="white"></text>
    </framed>
  </button>

  <framed id="textbox">
    <text col="white"></text>
  </framed>
  
  <framed id="doubletext">
    <line></line>
    <line></line>
    <line></line>
    <line inset="0.1" color="white;a:inv" size="10" order=",a"></line>
    <g hor="">
      <text color="white"></text>
      <text color="red"></text>
    </g>
  </framed>

  <framed id="f1">
    <line></line>
    <text color="white" size="l"></text>
  </framed>
  <framed id="f2">
    <line></line>
    <line></line>
    <line></line>
    <line inset="0.05" color="white"></line>
    <text color="white" size="xl"></text>
  </framed>
  <framed id="f3">
    <line></line>
    <line></line>
    <line inset="0.1" color="white"></line>
    <line></line>
    <text color="white" size="xl"></text>
  </framed>
  <checkbox id="cb">
    <framed>
      <box color="darkred"></box>
      <text col="white"></text>
    </framed>
    <framed>
      <box color="darkgreen"></box>
      <text col="white"></text>
    </framed>
  </checkbox>
</style>
